{
  "name": "smart-lib-mobile",
  "version": "0.1.0",
  "description": "Smart Library Management System - Mobile App",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "prebuild": "expo prebuild --clean",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-query/core": "^5.50.0",
    "@tanstack/react-query": "^5.50.0",
    "axios": "^1.7.7",
    "expo": "^51.0.28",
    "expo-barcode-scanner": "^12.16.1",
    "expo-camera": "^15.0.13",
    "expo-file-system": "^17.0.1",
    "expo-font": "^12.0.11",
    "expo-image-picker": "^15.0.7",
    "expo-notifications": "^0.28.4",
    "expo-router": "^3.5.20",
    "expo-secure-store": "^13.0.2",
    "expo-sharing": "^12.0.2",
    "expo-splash-screen": "^0.28.3",
    "expo-status-bar": "^1.13.0",
    "jose": "^5.7.0",
    "react": "18.3.1",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "^2.18.2",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "^3.16.4",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-screens": "^4.1.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "react-native",
    "expo",
    "library",
    "management",
    "system"
  ],
  "author": "Smart Library Team",
  "license": "MIT"
}
